{
  "name": "quicktalk-frontend-react",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" --names \"🦀,⚛️\" --prefix-colors \"yellow,cyan\"",
    "dev:backend": "cd ../backend && cargo watch -x run",
    "dev:frontend": "vite",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "tsc && vite build",
    "build:backend": "cd ../backend && cargo build --release",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "setup": "npm install && cd ../backend && cargo install cargo-watch"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.24.6",
    "axios": "^1.3.4",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "lucide-react": "^0.115.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "tailwindcss": "^3.2.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.21",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
